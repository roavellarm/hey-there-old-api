{
  "name": "hey-there-api",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "dev": "NODE_ENV=development nodemon --exec babel-node src/index.js",
    "lint": "./node_modules/.bin/eslint src",
    "build": "./node_modules/.bin/webpack --colors --progress",
    "watch": "yarn build -- --watch",
    "test": "./node_modules/.bin/mocha --recursive ./src --exit --require @babel/register",
    "tdd": "./node_modules/.bin/mocha --recursive ./src --exit --require @babel/register --watch",
    "coverage": "nyc yarn test && rm -rf .nyc_output"
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "*.spec.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "keywords": [],
